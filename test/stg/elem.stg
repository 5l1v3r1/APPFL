-- checks if an element is in a list of ints
-- elem :: Int -> List Int -> Bool 
elem = FUN(x y -> case y of {
       	       	       Nil -> false;
		       Cons h1 t1 -> let {pred = THUNK(eqInt x h1)} in case pred of {
		       	       	     	                                    True -> true;
					                                    False -> let {c = THUNK(elem x t1)} in c}});

-- list5: [5,4,3,2,1]
