--Gets element at top of stack
--top :: List a -> a
top = FUN(xs ->
             case xs of
               _scrt0 {
                 Cons h1 t1 -> h1 });

--Checks if stack is empty
--isEmpty :: List a -> Bool
isEmpty = FUN(xs ->
                 case xs of
                   _scrt1 {
                     Nil -> true;
                     Cons h1 t1 -> false });

