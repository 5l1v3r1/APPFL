-- unboxed fac 

fac = FUN(x ->
         case x of {
           I i# -> case wfac i# of { 
                      j# -> let {r = CON(I j#)} in r }});

wfac = FUN(i# ->
  case i# of {
    0 -> 1;
    j# -> case isub# i# 1 of {
       k# -> case wfac k# of {
         l# -> imul# i# l#  
       }
    }
 });   

--Test
output = THUNK (fac seven);
result = CON(I 5040);
main = THUNK(eqInt output result);
