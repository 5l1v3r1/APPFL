-- secondHalf
-- secondHalf a list, rounding down
-- secondHalf :: List a -> List a
secondHalf = FUN(xs ->
                    let {
                      l = THUNK(length xs);
                      a = THUNK(divInt l two);
                      final = THUNK(drop a xs)}
                    in final);

--merge :: List a -> List a -> a 
merge = FUN(xs ys ->
                  case xs of
                    _scrt2 {
                      Nil -> ys;
                      Cons h1 t1 -> case ys of
                                      _scrt1 {
                                        Nil -> xs;
                                        Cons h2 t2 -> let {
                                                        a = THUNK(intLE h2 h1)}
                                                      in case a of
                                                           _scrt0 {
                                                             True -> let {
                                                                       b = THUNK(merge xs t2);
                                                                       c = CON(Cons h2 b)}
                                                                     in c;
                                                             False -> let {
                                                                        b = THUNK(merge t1 ys);
                                                                        c = CON(Cons h1 b)}
                                                                      in c } } });

