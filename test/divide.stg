-- divide boxed Ints
-- divInt :: Int -> Int -> Int
-- Integer Division
divInt = FUN(x y -> case x of {
	  	     	  I i# -> case y of {
			       	       I j# -> case idiv# i# j# of {
				       	       	    x# -> let {result = CON(I x#)} in result }}});

--test
result = CON(I 2);
output = THUNK(divInt two one);
main = THUNK(eqInt result output);