
data Int = I Int#;

cd = 
  FUN(n -> case n of {
             I n# -> case cd# n# 0 of {
                          res# -> let {res = CON(I res#)}
			          in res }});
 
cd# = FUN(n# a# -> case n# of {
                     0 -> a#;
                     x# -> case iplus# a# 1 of {
                             ap# -> case isub# n# 1 of {
                                      np# -> cd# np# ap#
                                    }}});

ilarge = CON(I 1000000000);

main = THUNK(cd ilarge);
